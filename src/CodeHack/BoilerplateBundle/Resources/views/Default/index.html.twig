<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7 {% block htmlclass %}{% endblock htmlclass %}"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie10 lt-ie9 lt-ie8 {{ block('htmlclass')}}"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie10 lt-ie9 {{ block('htmlclass')}}"> <![endif]-->
<!--[if IE 9]>         <html class="no-js lt-ie10 {{ block('htmlclass')}}"> <![endif]-->
<!--[if gt IE 9]><!--> <html class="no-js no-ie {{ block('htmlclass')}}"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %}Fendi{% endblock %}</title>
        
        <meta name="description" content="{% block description %}Discover the Italian luxury fashion house of Fendi. Explore runway videos, interviews and browse the latest collections, handbags and accessories.{% endblock %}">
        
        <meta name="viewport" content="initial-scale=1.0, width=device-width,minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />

        {% block meta %}
            <meta property="og:title" content="FENDI">
            <meta property="og:description" content="Discover the Italian luxury fashion house of Fendi. Explore runway videos, interviews and browse the latest collections, handbags and accessories.">
            <meta property="og:type" content="website">
            <meta property="og:url" content="http://www.fendi.com/">
            <meta property="og:image" content="http://www.fendi.com/bundles/fasezero/img/profile.png">
        {% endblock meta %}
  

        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
        {% block stylesheets %} 
            <link rel="stylesheet" href="{{ asset('bundles/boilerplate/css/normalize.css') }}">
            <link rel="stylesheet" href="{{ asset('bundles/boilerplate/css/main.css') }}">
        {% endblock stylesheets %}
        
        {% block headjs %}
            <script src="{{ asset('bundles/boilerplate/js/vendor/modernizr-2.6.1.min.js') }}"></script>
        {% endblock headjs %}
    </head>
    <body data-responsejs='{
    "create": [
    { "breakpoints": [0,481,768,1025], "mode": "src", "prefix": "src" }, 
    { "breakpoints": [0,481,768,1025], "mode": "markup", "prefix": "r" },
    { "breakpoints": [1,1.5,2], "mode": "src", "prefix": "density", "prop": "device-pixel-ratio" }
    ]}'>

        {% block ie_alert %}
        <!--[if lte IE 7]>
        <div id="bg_outtodate">
            <p id="outtodate">
                <a class="alert_logo" href="/" title=""><img  data-src481="{{ asset('bundles/fasezero/img/h/logo.png') }}" src="{{ asset('bundles/fasezero/img/l/logo.png') }}" alt="Fendi"></a>
                Your browser is out to date.<br>
                Please update to the latest version of <a href="http://www.mozilla.com/firefox/">Firefox</a>,
                <a href="http://www.google.com/chrome/">Chrome</a>, <a href="http://www.apple.com/safari/">Safari</a>
                or <a href="http://www.microsoft.com/windows/internet-explorer/">Internet Explorer</a>.
            </p>
        </div>
        <![endif]-->
        {% endblock ie_alert %}

        {% block body %}
            <!-- Add your site or application content here -->
            <p>Hello world! This is Fendi.</p>
        {% endblock body %}

        {% block javascripts %}
            <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
            <script>window.jQuery || document.write('<script src="{{ asset('bundles/boilerplate/js/vendor/jquery-1.8.0.min.js') }}"><\/script>')</script>
            <script src="{{ asset('bundles/boilerplate/js/plugins.js') }}"></script>
            <script src="{{ asset('bundles/boilerplate/js/main.js') }}"></script>
        {% endblock javascripts %}

        {% block analytics_js %}
            <!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
            <script>
                var _gaq=[['_setAccount','UA-XXXXX-X'],['_trackPageview']];
                (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
                g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
                s.parentNode.insertBefore(g,s)}(document,'script'));
            </script>
        {% endblock analytics_js %}
    </body>
</html>

